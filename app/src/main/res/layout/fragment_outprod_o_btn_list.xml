<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragment.outprodO.outprodOBtnListFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/top_bar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/bg_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/outprodOBtnList_home_btn"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:paddingVertical="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/home_btn_img"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="15dp"
            android:background="@drawable/house_solid2"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/outprodOBtnList_tx_top"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:fontFamily="@font/nanumsquareneo_deb"
            android:gravity="center"
            android:text="해외 출고"
            android:textColor="@color/white"
            android:textSize="@dimen/h5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <RelativeLayout
        android:id="@+id/outprodOBtnList_bt_out_ly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/top_bar"
        app:layout_constraintVertical_bias="0.05"
        >


        <RelativeLayout
            android:id="@+id/relative_layout_outprodOBtnList_bt_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/outprodOBtnList_bt_packing"
                android:layout_width="150dp"
                android:layout_height="@dimen/main_btn_h_size"
                android:paddingHorizontal="30dp"
                android:background="@drawable/box_shape_round"
                android:text="패킹 지시"
                android:textStyle="bold"
                android:textSize="15sp"
                />
        </RelativeLayout>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/outprodOBtnList_bt_ly2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/outprodOBtnList_bt_out_ly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/outprodOBtnList_bt_out_ly"
        app:layout_constraintVertical_bias="0.1">

        <RelativeLayout
            android:id="@+id/relative_layout_outprodOBtnList_bt_B"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/outprodOBtnList_bt_case_list"
                android:layout_width="150dp"
                android:layout_height="@dimen/main_btn_h_size"
                android:background="@drawable/box_shape_round"
                android:paddingHorizontal="5dp"
                android:text="반포장 목록"
                android:textSize="15sp"
                android:textStyle="bold" />


        </RelativeLayout>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/outprodOBtnList_bt_ly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@id/guideline2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/outprodOBtnList_bt_out_ly"
        >

        <RelativeLayout
            android:id="@+id/relative_layout_outprodOBtnList_bt_A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/outprodOBtnList_bt_out"
                android:layout_width="150dp"
                android:layout_height="@dimen/main_btn_h_size"
                android:paddingHorizontal="30dp"
                android:background="@drawable/box_shape_round"
                android:text="포장 이동"
                android:textStyle="bold"
                android:textSize="15sp"
                />


        </RelativeLayout>

    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>